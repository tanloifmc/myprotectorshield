{
  "version": 2,
  "builds": [
    {
      "src": "myprotector-landing/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "myprotector-shield/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    { "source": "/app/(.*)", "destination": "/myprotector-shield/dist/$1" },
    { "source": "/docs/(.*)", "destination": "/docs/$1" },
    { "source": "/(.*)", "destination": "/myprotector-landing/dist/$1" }
  ]
}
